{
  "name": "optimize-prime",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "@material-ui/pickers": "^3.2.6",
    "@material-ui/styles": "^4.5.0",
    "@microsoft/microsoft-graph-client": "^2.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.4.1",
    "dotenv": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mysql": "^2.17.1",
    "node-forge": "^0.9.1",
    "parcel-bundler": "^1.12.4",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-input-mask": "^3.0.0-alpha.0",
    "react-moment": "^0.9.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-svg": "^10.0.25",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sweetalert2": "^8.18.4",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start": "cd ./server/src && node ./start.js",
    "start:client": "rimraf ./cache && rimraf ./dist/client && parcel ./client/public/index.html --port 3010 --hmr-port 3011 --out-dir ./dist/client",
    "build": "rimraf ./dist/client && parcel build ./client/public/index.html --out-dir ./dist/client",
    "test": "jest",
    "eject": "react-scripts eject",
    "server": "node server/src/start.js",
    "test:client": "jest ./client/src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./client/src/__test__/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/src/__mocks__/styleMock.js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  }
}
